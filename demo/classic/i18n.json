{
  "version": 1,
  "debug": false,
  "locale": {
    "source": "en",
    "targets": [
      "es"
    ]
  },
  "content": [
    {
      "type": "json",
      "path": "json/[lang].json"
    },
    {
      "type": "xcode",
      "path": "xcode/Localizable.xcstrings"
    },
    {
      "type": "yaml",
      "path": "yaml/[lang].yml"
    },
    {
      "type": "yaml-root-key",
      "path": "yaml-root-key/[lang].yml"
    },
    {
      "type": "markdown",
      "path": "markdown/[lang].md"
    }
  ]
}
